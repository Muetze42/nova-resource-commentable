{
    "name": "norman-huth/nova-resource-commentable",
    "description": "Add comments to Nova resources",
    "keywords": [
        "laravel",
        "nova",
        "tool"
    ],
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.10.00",
        "norman-huth/nova-hooks": "^1.0.0-beta.4^|^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "NormanHuth\\NovaResourceCommentable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\NovaResourceCommentable\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.14"
    }
}
